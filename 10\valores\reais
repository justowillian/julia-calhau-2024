#include <iostream>
using namespace std;

int main() {
    const int TAMANHO = 10; 
    double valores[TAMANHO]; 

    cout << "Digite 10 valores reais:\n";

    for (int i = 0; i < TAMANHO; ++i) {
        cout << "Valor " << i + 1 << ": ";
        cin >> valores[i];
    }

    cout << "Valores na ordem inversa de entrada:\n";
    for (int i = TAMANHO - 1; i >= 0; --i) {
        cout << valores[i] << " ";
    }
    cout << endl;

    return 0;
}
