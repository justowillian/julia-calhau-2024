#include <iostream>
using namespace std;

int main() {
    const int TAMANHO = 5;
    int valores[TAMANHO]; 
    int soma = 0; 

    cout << "Digite 5 valores inteiros:\n";

    for (int i = 0; i < TAMANHO; ++i) {
        cout << "Valor " << i + 1 << ": ";
        cin >> valores[i];
        soma += valores[i];
    }

    cout << "A soma dos valores Ã©: " << soma << endl;

    return 0;
}
